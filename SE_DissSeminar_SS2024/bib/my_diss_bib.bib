}

@article{piontti2014,
  title = {The Infection Tree of Global Epidemics},
  author = {Piontti, Ana Pastore Y. and Gomes, Marcelo Ferreira Da Costa and Samay, Nicole and Perra, Nicola and Vespignani, Alessandro},
  date = {2014-04},
  journaltitle = {Network Science},
  volume = {2},
  pages = {132--137},
  issn = {2050-1242, 2050-1250},
  doi = {10.1017/nws.2014.5},
  url = {https://www.cambridge.org/core/journals/network-science/article/the-infection-tree-of-global-epidemics/485C6A9D71772C5D9B8CAAFC4795D3FD},
  urldate = {2018-06-01},
  abstract = {The spreading of transmissible infectious diseases is inevitably entangled with the dynamics of human population. Humans are the carrier of the pathogen, and the large-scale travel and commuting patterns that govern the mobility of modern societies are defining how epidemics and pandemics travel across the world. For a long time, the development of quantitative spatially explicit models able to shed light on the global dynamics of pandemic has been limited by the lack of detailed data on human mobility. In the last 10 years, however, these limits have been lifted by the increasing availability of data generated by new information technologies, thus triggering the development of computational (microsimulation) models working at a level of single individuals in spatially extended regions of the world. Microsimulations can provide information at very detailed spatial resolutions and down to the level of single individuals. In addition, computational implementations explicitly account for stochasticity, allowing the study of multiple realizations of epidemics with the same parameters' distribution. While on the one hand these capabilities represent the richness of microsimulation methods, on the other hand they face us with a huge amount of information that requires the use of specific data reduction methods and visual analytics.},
  file = {C\:\\Users\\lukas07\\Zotero\\storage\\S5WN6ZCK\\Piontti et al. - 2014 - The infection tree of global epidemics.pdf;C\:\\Users\\lukas07\\Zotero\\storage\\4RNUEN5U\\485C6A9D71772C5D9B8CAAFC4795D3FD.html},
  langid = {english},
  number = {1}
}

