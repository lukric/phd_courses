  eprinttype = {pmid},
  keywords = {Adolescent,Child; Preschool,England,Female,Humans,Infant,Intervention,Mathematical model,Models; Theoretical,Pertussis,Pertussis Vaccine,Resurgence,Transmission,Vaccination,Vaccine,Wales,Whooping Cough},
