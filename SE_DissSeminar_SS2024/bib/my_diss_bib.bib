@article{domenechdecelles2016,
  title = {The Pertussis Enigma: Reconciling Epidemiology, Immunology and Evolution},
  shorttitle = {The Pertussis Enigma},
  author = {Domenech de Cellès, Matthieu and Magpantay, Felicia M. G. and King, Aaron A. and Rohani, Pejman},
  date = {2016-01-13},
  journaltitle = {Proceedings. Biological Sciences},
  shortjournal = {Proc. Biol. Sci.},
  volume = {283},
  issn = {1471-2954},
  doi = {10.1098/rspb.2015.2309},
  abstract = {Pertussis, a highly contagious respiratory infection, remains a public health priority despite the availability of vaccines for 70 years. Still a leading cause of mortality in developing countries, pertussis has re-emerged in several developed countries with high vaccination coverage. Resurgence of pertussis in these countries has routinely been attributed to increased awareness of the disease, imperfect vaccinal protection or high infection rates in adults. In this review, we first present 1980-2012 incidence data from 63 countries and show that pertussis resurgence is not universal. We further argue that the large geographical variation in trends probably precludes a simple explanation, such as the transition from whole-cell to acellular pertussis vaccines. Reviewing available evidence, we then propose that prevailing views on pertussis epidemiology are inconsistent with both historical and contemporary data. Indeed, we summarize epidemiological evidence showing that natural infection and vaccination both appear to provide long-term protection against transmission and disease, so that previously infected or vaccinated adults contribute little to overall transmission at a population level. Finally, we identify several promising avenues that may lead to a consistent explanation of global pertussis epidemiology and to more effective control strategies.},
  eprint = {26763701},
  eprinttype = {pmid},
  file = {C\:\\Users\\lukas07\\Zotero\\storage\\5WUS62YR\\Domenech de Cellès et al. - 2016 - The pertussis enigma reconciling epidemiology, im.pdf},
  keywords = {Adult,Bordetella pertussis,Communicable Diseases; Emerging,Disease Reservoirs,Humans,Incidence,Infant,Infant; Newborn,infection-derived immunity,Mass Vaccination,pertussis,pertussis epidemiology,pertussis resurgence,pertussis vaccines,vaccine-derived immunity,Whooping Cough},
  langid = {english},
  number = {1822},
