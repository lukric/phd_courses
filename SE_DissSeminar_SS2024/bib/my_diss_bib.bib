  author = {Cárcamo, César P. and Campos, Pablo E. and García, Patricia J. and Hughes, James P. and Garnett, Geoff P. and Holmes, King K. and {Peru PREVEN study team}},
  date = {2012-10},
  journaltitle = {The Lancet. Infectious Diseases},
  volume = {12},
