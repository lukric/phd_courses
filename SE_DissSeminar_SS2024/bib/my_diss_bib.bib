  keywords = {Carrier State,Europe,Heptavalent Pneumococcal Conjugate Vaccine,Humans,Nasopharynx,Pneumococcal Infections,Pneumococcal Vaccines,Prevalence,Streptococcus pneumoniae,Vaccination},
