}

@article{pascal2006,
  title = {France's Heat Health Watch Warning System},
  author = {Pascal, Mathilde and Laaidi, Karine and Ledrans, Martine and Baffert, Elsa and Caserio-Schönemann, Céline and Le Tertre, Alain and Manach, Jacques and Medina, Sylvia and Rudant, Jérémie and Empereur-Bissonnet, Pascal},
  date = {2006-01},
