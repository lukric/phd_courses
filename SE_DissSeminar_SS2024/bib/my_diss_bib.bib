  eprinttype = {pmid},
  keywords = {Acute Disease,Heptavalent Pneumococcal Conjugate Vaccine,Humans,Infant,Otitis Media,Pneumococcal Infections,Pneumococcal Vaccines,Pneumonia; Pneumococcal,Randomized Controlled Trials as Topic,Streptococcus pneumoniae,Treatment Outcome},
