@article{kuo2011,
  title = {Influenza-Related Excess Mortality, {{Austria}} 2001 till 2009},
  author = {Kuo, Hung-Wei and Schmid, Daniela and Liu, Yu-Lun and Lachner, Peter and Allerberger, Franz},
  date = {2011-10-01},
  journaltitle = {Wiener klinische Wochenschrift},
  shortjournal = {Wien Klin Wochenschr},
  volume = {123},
