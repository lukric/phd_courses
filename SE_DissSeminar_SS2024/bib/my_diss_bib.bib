@article{schmid2011,
  title = {Foodborne Gastroenteritis Outbreak in an {{Austrian}} Healthcare Facility Caused by Asymptomatic, Norovirus-Excreting Kitchen Staff},
  author = {Schmid, D. and Kuo, H.-W. and Hell, M. and Kasper, S. and Lederer, I. and Mikula, C. and Springer, B. and Allerberger, F.},
