@article{debarros2019,
  title = {Pertussis Epidemiological Pattern and Disease Burden in {{Brazil}}: An Analysis of National Public Health Surveillance Data},
  shorttitle = {Pertussis Epidemiological Pattern and Disease Burden in {{Brazil}}},
  author = {De Barros, Eliana Nogueira Castro and Nunes, Altacilio Aparecido and Abreu, Ariane De Jesus Lopes De and Furtado, Bárbara Emoingt and Cintra, Otavio and Cintra, Monica Act and Coelho, Eduardo Barbosa},
