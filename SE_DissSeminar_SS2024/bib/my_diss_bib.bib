  issue = {5 Suppl},
  keywords = {Communicable Disease Control,Female,Follow-Up Studies,Humans,Immunity; Innate,Male,Pertussis Vaccine,Sensitivity and Specificity,Time Factors,Vaccination,Whooping Cough},
  langid = {english}
}

