  shorttitle = {Carriage Prevalence and Serotype Distribution of {{Streptococcus}} Pneumoniae Prior to 10-Valent Pneumococcal Vaccine Introduction},
  author = {Nackers, Fabienne and Cohuet, Sandra and le Polain de Waroux, Olivier and Langendorf, Céline and Nyehangane, Dan and Ndazima, Donny and Nanjebe, Deborah and Karani, Angela and Tumwesigye, Elioda and Mwanga-Amumpaire, Juliet and Scott, J. Anthony G. and Grais, Rebecca F.},
