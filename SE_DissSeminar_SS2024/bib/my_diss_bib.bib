  eprinttype = {pmid},
  file = {C\:\\Users\\lukas07\\Zotero\\storage\\8HD95CMW\\schaefer2018 - suppl2 journal.pone.0200750.s002.docx;C\:\\Users\\lukas07\\Zotero\\storage\\E82IGG8K\\Schaefer2018 - Disease burden of hepatitis C in the Austrian state of Tyrol.pdf;C\:\\Users\\lukas07\\Zotero\\storage\\W8ZVF8EN\\schaefer2018 - suppl1 journal.pone.0200750.s001.docx},
  keywords = {Adolescent,Adult,Age Factors,Aged,Aged; 80 and over,Austria,Carcinoma; Hepatocellular,Cost of Illness,Female,Genotype,Hepacivirus,Hepatitis C; Chronic,Humans,Liver Cirrhosis,Liver Neoplasms,Male,Middle Aged,Prevalence,Viremia,World Health Organization,Young Adult},
