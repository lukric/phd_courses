  eprinttype = {pmid},
  file = {C\:\\Users\\lukas07\\Zotero\\storage\\DXET3UPF\\Huang 2011-Projecting Future Heat-Related Mortality under Climate Change.pdf},
  keywords = {Climate change,Forecasting,Hot Temperature,Humans,Models; Theoretical,Research},
  langid = {english},
