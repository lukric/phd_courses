@article{euhcv2017,
  title = {Hepatitis {{C}} Virus Prevalence and Level of Intervention Required to Achieve the {{WHO}} Targets for Elimination in the {{European Union}} by 2030: A Modelling Study},
  shorttitle = {Hepatitis {{C}} Virus Prevalence and Level of Intervention Required to Achieve the {{WHO}} Targets for Elimination in the {{European Union}} by 2030},
  author = {{European Union HCV Collaborators}},
  date = {2017-05},
  journaltitle = {The Lancet. Gastroenterology \& Hepatology},
  shortjournal = {Lancet Gastroenterol Hepatol},
  volume = {2},
  pages = {325--336},
  issn = {2468-1253},
  doi = {10.1016/S2468-1253(17)30045-6},
  abstract = {BACKGROUND: Hepatitis C virus (HCV) is a leading cause of liver-related morbidity and mortality worldwide. In the European Union (EU), treatment and cure of HCV with direct-acting antiviral therapies began in 2014. WHO targets are to achieve a 65\% reduction in liver-related deaths, a 90\% reduction of new viral hepatitis infections, and 90\% of patients with viral hepatitis infections being diagnosed by 2030. This study assessed the prevalence of HCV in the EU and the level of intervention required to achieve WHO targets for HCV elimination.
METHODS: We populated country Markov models for the 28 EU countries through a literature search of PubMed and Embase between Jan 1, 2000, and March 31, 2016, and a Delphi process to gain expert consensus and validate inputs. We aggregated country models to create a regional EU model. We used the EU model to forecast HCV disease progression (considering the effect of immigration) and developed a strategy to acehive WHO targets. We used weighted average sustained viral response rates and fibrosis restrictions to model the effect of current therapeutic guidelines. We used the EU model to forecast HCV disease progression (considering the effect of immigration) under current screening and therapeutic guidelines. Additionally, we back-calculated the total number of patients needing to be screened and treated to achieve WHO targets.
FINDINGS: We estimated the number of viraemic HCV infections in 2015 to be 3\hphantom{,}238\hphantom{,}000 (95\% uncertainty interval [UI] 2\hphantom{,}106\hphantom{,}000-3\hphantom{,}795\hphantom{,}000) of a total population of 509\hphantom{,}868\hphantom{,}000 in the EU, equating to a prevalence of viraemic HCV of 0·64\% (95\% UI 0·41-0·74). We estimated that 1\hphantom{,}180\hphantom{,}000 (95\% UI 1\hphantom{,}003\hphantom{,}000-1\hphantom{,}357\hphantom{,}000) people were diagnosed with viraemia (36·4\%), 150\hphantom{,}000 (12\hphantom{,}000-180\hphantom{,}000) were treated (4·6\% of the total infected population or 12·7\% of the diagnosed population), 133\hphantom{,}000 (106\hphantom{,}000-160\hphantom{,}000) were cured (4·1\%), and 57\hphantom{,}900 (43\hphantom{,}900-67\hphantom{,}300) were newly infected (1·8\%) in 2015. Additionally, 30\hphantom{,}400 (26\hphantom{,}600-42\hphantom{,}500) HCV-positive immigrants entered the EU. To achieve WHO targets, unrestricted treatment needs to increase from 150\hphantom{,}000 patients in 2015 to 187\hphantom{,}000 patients in 2025 and diagnosis needs to increase from 88\hphantom{,}800 new cases annually in 2015 to 180\hphantom{,}000 in 2025.
INTERPRETATION: Given its advanced health-care infrastructure, the EU is uniquely poised to eliminate HCV; however, expansion of screening programmes is essential to increase treatment to achieve the WHO targets. A united effort, grounded in sound epidemiological evidence, will also be necessary.
FUNDING: Gilead Sciences.},
  eprint = {28397696},
  eprinttype = {pmid},
  file = {C\:\\Users\\lukas07\\Zotero\\storage\\MAXBXGM4\\European Union HCV Collaborators - 2017 - Hepatitis C virus prevalence and level of interven.pdf},
  keywords = {Antiviral Agents,Cost of Illness,Disease Eradication,Emigration and Immigration,European Union,Hepatitis C,Humans,Markov Chains,Prevalence,Viremia,World Health Organization},
  langid = {english},
  number = {5}
}

