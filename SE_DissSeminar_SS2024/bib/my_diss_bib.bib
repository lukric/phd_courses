@article{tan2012,
  title = {Pediatric Invasive Pneumococcal Disease in the {{United States}} in the Era of Pneumococcal Conjugate Vaccines},
  author = {Tan, Tina Q.},
  date = {2012-07},
  journaltitle = {Clinical Microbiology Reviews},
