  eprinttype = {pmid},
  keywords = {Bordetella pertussis,History; 20th Century,History; 21st Century,Humans,Immunization,Immunization; Secondary,Incidence,Infection,Pertussis vaccine,Pertussis Vaccine,Prevention,Prophylaxis,Treatment Outcome,Whooping cough,Whooping Cough},
