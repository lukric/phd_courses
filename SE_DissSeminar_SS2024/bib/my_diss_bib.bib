  title = {Surveillance of {{Summer Mortality}} and {{Preparedness}} to {{Reduce}} the {{Health Impact}} of {{Heat Waves}} in {{Italy}}},
  author = {Michelozzi, Paola and de’ Donato, Francesca K. and Bargagli, Anna Maria and D’Ippoliti, Daniela and De Sario, Manuela and Marino, Claudia and Schifano, Patrizia and Cappai, Giovanna and Leone, Michela and Kirchmayer, Ursula and Ventura, Martina and di Gennaro, Marta and Leonardi, Marco and Oleari, Fabrizio and De Martino, Annamaria and Perucci, Carlo A.},
