  author = {Galanis, Ilias and Lindstrand, Ann and Darenberg, Jessica and Browall, Sarah and Nannapaneni, Priyanka and Sjöström, Karin and Morfeldt, Eva and Naucler, Pontus and Blennow, Margareta and Örtqvist, Åke and Henriques-Normark, Birgitta},
