  eprinttype = {pmid},
  keywords = {Bacteremia,Case-Control Studies,Case–control study,Child; Preschool,Effectiveness,Female,Humans,Infant,invasive pneumococcal disease,Male,Meningitis; Pneumococcal,pneumococcal conjugate vaccine,Pneumococcal Infections,Pneumococcal Vaccines,Quebec,Treatment Outcome,Vaccines; Conjugate},
