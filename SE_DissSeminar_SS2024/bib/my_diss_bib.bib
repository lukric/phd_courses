  eprinttype = {pmid},
  keywords = {Case-Control Studies,Child; Preschool,Female,Humans,Infant,Male,Mass Vaccination,Pneumococcal Infections,Pneumococcal Vaccines,Registries,Treatment Outcome,United States},
