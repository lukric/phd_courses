  keywords = {Adolescent,Adult,Antibodies,Chlamydia Infections,Female,Gonorrhea,Health Surveys,Herpes Simplex,Herpesvirus 2,HIV Infections,HTLV-I Infections,Human,Humans,Male,Peru,Prevalence,Prostitution,Questionnaires,Sexual Behavior,Sexually Transmitted Diseases,Syphilis,Trichomonas Vaginitis,Urban Population,Vagina,Viral,Young Adult},
